BUBBLE SORT:

#include <stdio.h>
#include<stdlib.h>

int main() {
    int arr[] = {2,7,8,9,10,23};
    int n = sizeof(arr)/sizeof(arr[0]);
    
    for(int i=0;i<n-1;i++)
    {
        int flag = 0;
        for(int j=0;j<n-1-i;j++)
        {
            if(arr[j]>arr[j+1])
            {
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
                flag = 1;
            }
        }
        if(flag == 0)
           break;
    }
    
    printf("Sorted Array:");
    for(int i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
}
